# AWS S3 Configuration
AWS_REGION=us-west-2
S3_BUCKET=your-bucket-name
# AWS_ACCESS_KEY_ID=... (set in environment or use IAM role)
# AWS_SECRET_ACCESS_KEY=... (set in environment or use IAM role)

# Gemini API (optional - uses fallback if not set)
GEMINI_API_KEY=

# Database (optional for now)
# DATABASE_URL=postgresql://user:password@localhost:5432/csvnest

# JWT (for authentication)
JWT_SECRET=your-secret-key-here

# Server Configuration
PORT=4000
NODE_ENV=development

# Frontend API Base (for CORS, etc.)
FRONTEND_URL=http://localhost:5173

# Email Configuration (for notifications - optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SENDER_EMAIL=noreply@csvnest.com

